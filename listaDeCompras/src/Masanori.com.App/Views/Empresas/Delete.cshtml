@model Masanori.com.App.ViewModels.EmpresaViewComponent

@{
    ViewData["Title"] = "Deletar Empresa " + Model.Nome;
}

    <div style="padding-top: 20px">
        <h4>@ViewData["Title"]</h4>
        <h5>Esta ação é permanente!</h5>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nome)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Cnpj)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cnpj)
            </dd>
        </dl>

        <div id="EnderecoTarget">
            <partial name="_DetalhesEndereco" />
        </div>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a class="btn btn-warning" asp-action="Index">Voltar</a>
        </form>

    </div>
